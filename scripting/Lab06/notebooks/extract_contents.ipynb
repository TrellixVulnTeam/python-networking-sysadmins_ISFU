    "import tarfile\n",
    "import os\n",
    "\n",
    "try:\n",
    "    os.mkdir('work')\n",
    "except FileExistsError:\n",
    "    # directory already exists\n",
    "    pass\n",
    "\n",
    "with tarfile.open('work.tar', 'r') as t:\n",
    "\tt.extractall('work')\n",
    "print(os.listdir('work'))\n"
