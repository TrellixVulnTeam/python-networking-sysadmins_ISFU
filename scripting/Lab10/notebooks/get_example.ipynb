      "source": [
        "import http.client\n",
        "\n",
        "con_obj = http.client.HTTPSConnection(\"www.imdb.com\")\n",
        "con_obj.request(\"GET\", \"/\")\n",
        "response = con_obj.getresponse()\n",
        "print(\"Status: {}\".format(response.status))\n",
        "\n",
        "read_data = response.read(1000)\n",
        "print(read_data)\n",
        "con_obj.close()\n"
      ],
