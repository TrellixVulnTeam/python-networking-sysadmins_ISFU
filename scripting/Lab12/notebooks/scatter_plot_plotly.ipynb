      "source": [
        "import plotly\n",
        "import plotly.graph_objs as go\n",
        "import numpy as np\n",
        "\n",
        "x_axis = np.random.randn(100)\n",
        "y_axis = np.random.randn(100)\n",
        "\n",
        "trace = go.Scatter(x=x_axis, y=y_axis, mode = 'markers')\n",
        "      \n",
        "data_set = [trace]\n",
        "\n",
        "plotly.offline.plot(data_set, filename='scatter_plot')\n",
        "\n"
      ],
