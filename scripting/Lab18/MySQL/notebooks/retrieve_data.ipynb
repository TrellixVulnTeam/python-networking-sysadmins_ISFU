        "import MySQLdb as mdb\n",
        "\n",
        "con_obj = mdb.connect('localhost', 'test_user', 'test123', 'test')\n",
        "\n",
        "with con_obj: \n",
        "\n",
        "\tcur_obj = con_obj.cursor()\n",
        "\tcur_obj.execute(\"SELECT * FROM books\")\n",
        "\n",
        "\trecords = cur_obj.fetchall()\n",
        "\n",
        "\tfor r in records:\n",
        "\t\tprint(r)\n",
        "\n"
