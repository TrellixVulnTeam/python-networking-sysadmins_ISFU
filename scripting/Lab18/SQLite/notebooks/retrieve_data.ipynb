        "import sqlite3\n",
        "\n",
        "con_obj = sqlite3.connect('test.db')\n",
        "\n",
        "cur_obj = con_obj.execute(\"SELECT title, author from books\")\n",
        "for row in cur_obj:\n",
        "\tprint (\"Title = \", row[0])\n",
        "\tprint (\"Author = \", row[1], \"\\n\")\n",
        "\n",
        "con_obj.close()\n",
        "\n"
